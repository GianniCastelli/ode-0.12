# raycar.scn

car {
    type car
    model raycar-unified.bin
    x -35
    y 25
    z 1
    name car
}

track {
    type obstacle
    model track1.bin
    x 0
    y 0
    z 0
    name track
}

crate {
    type obstacle
    model crate1.bin
    x -6
    y -1
    z 1
    name crate
}

maincam {
    type followcamera
    x -3
    y 11
    z 3
    fovy 60
    name maincam
    target car
    distance 10
}

