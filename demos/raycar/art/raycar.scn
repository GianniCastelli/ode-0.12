# raycar.scn

car {
    type car
    model raycar-unified.bin
    x -3
    y -3
    z 1
    name car
}

track {
    type obstacle
    model track1.bin
    x 0
    y 0
    z 0
    name track
}

crate {
    type obstacle
    model crate1.bin
    x -6
    y -1
    z 1
    name crate
}

maincam {
    type camera
    x -10
    y -5
    z 2
    lx 0
    ly 0
    lz 1
    fovy 60
    name maincam
}

